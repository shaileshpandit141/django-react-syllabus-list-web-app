{
    "builds": [
        {
            "src": "frontend/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/wsgi.py"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}